{
  "metadata": {
    "description": "Comprehensive data sources for Washington State fiscal transparency",
    "lastUpdated": "2025-11-26",
    "reconciliationMethod": "weighted-average",
    "confidenceThreshold": 0.7
  },
  "dataSources": {
    "budget_total": {
      "description": "Total state biennial budget",
      "unit": "USD",
      "sources": [
        {
          "id": "ofm_budget_2024",
          "name": "WA Office of Financial Management - Enacted Budget 2024-2025",
          "url": "https://dor.wa.gov/taxes-rates/tax-information/budget",
          "apiUrl": "https://data.wa.gov/api/3/action/datastore_search?resource_id=budget-2024&limit=1000",
          "format": "json",
          "field": "total_budget",
          "weight": 1.0,
          "frequency": "annual",
          "lastFetched": null
        },
        {
          "id": "leap_bill_fiscal_notes",
          "name": "LEAP (Legislative Evaluation & Accountability Program)",
          "url": "https://leap.leg.wa.gov/",
          "apiUrl": "https://api.leg.wa.gov/leap",
          "format": "json",
          "field": "projected_impact.total",
          "weight": 0.8,
          "frequency": "daily",
          "lastFetched": null
        },
        {
          "id": "treasury_report",
          "name": "WA State Treasurer Financial Reports",
          "url": "https://tre.wa.gov/financial-reporting/",
          "apiUrl": "https://tre.wa.gov/api/financial-reports",
          "format": "json",
          "field": "biennial_budget",
          "weight": 0.9,
          "frequency": "quarterly",
          "lastFetched": null
        }
      ],
      "reconciliation": {
        "method": "weighted-average",
        "fallback": "ofm_budget_2024"
      }
    },
    "population_total": {
      "description": "Total Washington state population",
      "unit": "persons",
      "sources": [
        {
          "id": "census_2020",
          "name": "US Census Bureau 2020 Decennial Census",
          "url": "https://www.census.gov/quickfacts/WA",
          "apiUrl": "https://api.census.gov/data/2020/dec/dhc?get=PCT10001N&for=state:53",
          "format": "json",
          "field": "population",
          "weight": 1.0,
          "frequency": "decennial",
          "lastFetched": null
        },
        {
          "id": "census_acs_1yr",
          "name": "US Census ACS 1-Year Estimates (most recent)",
          "url": "https://www.census.gov/programs-surveys/acs/data.html",
          "apiUrl": "https://api.census.gov/data/2022/acs/acs1?get=B01003_001E&for=state:53",
          "format": "json",
          "field": "total_population",
          "weight": 0.95,
          "frequency": "annual",
          "lastFetched": null
        },
        {
          "id": "ofw_population",
          "name": "Office of Forecast and Research - WA Population Estimates",
          "url": "https://ofm.wa.gov/washington-data-research/population-demography/population-estimate",
          "apiUrl": "https://ofm.wa.gov/api/population-estimate",
          "format": "json",
          "field": "total",
          "weight": 0.98,
          "frequency": "annual",
          "lastFetched": null
        }
      ],
      "reconciliation": {
        "method": "weighted-average",
        "fallback": "ofw_population"
      }
    },
    "debt_total": {
      "description": "Total Washington state debt (general obligation bonds, revenue bonds)",
